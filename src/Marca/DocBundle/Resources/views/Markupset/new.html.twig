{# Marca/DocBundle/Resources/views/Markupset/new.html.twig #}

{% extends 'MarcaUserBundle::dialog.html.twig' %}

{% block content %}
<div class="col-sm-11 center">
    <form action="{{ path('markupset_create') }}" method="post" {{ form_enctype(form) }}>

        <div class="panel panel-default">
            <div class="panel-heading">
                <h2 class="panel-title small">Markup Set</h2>
            </div>
            <div class="panel-body">
                {{ form_widget(form) }}
            </div>
            <div class="panel-footer">
                <div class="btn-group">
                    <button class="btn btn-primary" type="submit">Update</button>
                    <a class="btn btn-default" href="{{ path('markupset') }}">Cancel</a>
                </div>
            </div>
        </div>
    </form>
</div>
{% endblock %}