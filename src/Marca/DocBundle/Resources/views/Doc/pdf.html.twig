<style>

body {
	font-family: 'sans-serif';
	margin-right: .5in;
	margin-left: .5in;
}

.center {
    float: none;
    margin-left:auto; margin-right: auto;
}

.eDoc_note {
    color:blue;
    background:white;
    border:#e3e3e3 1px solid;
    border-radius: 0 4px 4px 0;
    margin-left:-1px;
    margin-right: 4px;
    padding:2px 2px 2px 6px;
}

.eDoc_highlight {
    background:#FFFFD3;
    padding:2px 2px 2px 6px;
    border:#e3e3e3 1px solid;
    border-radius: 4px 0 0 4px;
}

.eDoc_hanging_indent {
    text-indent:-0.5in;
    margin-left:0.5in;
    line-height: 200%;
}

.eDoc_paragraph_indent {
    text-indent:0.5in;
    line-height: 200%;
}

.eDoc_single_space {
    line-height: 100%;
}

.eDoc_double_space {
    line-height: 200%;
}

.eDoc_super {
    color:blue;
    vertical-align:super;
    font-size:smaller;
    line-height: normal;
}
.eDoc_title_pdf {
	text-align: center;
	
}
.eDoc_wc_pdf {
	text-align: center;
	page-break-before: always;	
}
.eDoc_header_pdf {
	text-align: left;
    line-height: 200%;
}

.markup  {
    padding: 2px 4px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

