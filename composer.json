{
    "name": "calliopeinitiative/marca",
    "license": "GPL",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Marca\\AdminBundle\\": "src/Marca/AdminBundle",
            "Marca\\AssessmentBundle\\": "src/Marca/AssessmentBundle",
            "Marca\\AssignmentBundle\\": "src/Marca/AssignmentBundle",
            "Marca\\CalendarBundle\\": "src/Marca/CalendarBundle",
            "Marca\\CourseBundle\\": "src/Marca/CourseBundle",
            "Marca\\DocBundle\\": "src/Marca/DocBundle",
            "Marca\\FileBundle\\": "src/Marca/FileBundle",
            "Marca\\ForumBundle\\": "src/Marca/ForumBundle",
            "Marca\\GradebookBundle\\": "src/Marca/GradebookBundle",
            "Marca\\HomeBundle\\": "src/Marca/HomeBundle",
            "Marca\\JournalBundle\\": "src/Marca/JournalBundle",
            "Marca\\NoteBundle\\": "src/Marca/NoteBundle",
            "Marca\\PortfolioBundle\\": "src/Marca/PortfolioBundle",
            "Marca\\ResponseBundle\\": "src/Marca/ResponseBundle",
            "Marca\\TagBundle\\": "src/Marca/TagBundle",
            "Marca\\UserBundle\\": "src/Marca/UserBundle"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.2",
        "symfony/symfony": "3.4.*",
        "doctrine/orm": "^2.6.3",
        "doctrine/doctrine-bundle": "~1.10.2",
        "symfony/swiftmailer-bundle": "~3.2.5",
        "symfony/monolog-bundle": "~3.3",
        "sensio/framework-extra-bundle": "^5.2.4",
        "incenteev/composer-parameter-handler": "~2.1",
        "friendsofsymfony/user-bundle": "~2.1",
        "stof/doctrine-extensions-bundle": "~1.3",
        "knplabs/knp-paginator-bundle": "~3.0",
        "besimple/sso-auth-bundle": "*@dev",
        "egeloen/ckeditor-bundle": "^6.0",
        "vich/uploader-bundle": "~1.9",
        "dompdf/dompdf": "^0.8.3",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "sensio/distribution-bundle": "^5.0"
    },
    "require-dev": {
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.4-dev"
        }
    }
}
